				<img src="Images/TechSurge.png"/>
			</div>

			<div class="col-sm-6">
				<div class="form">
		
					<div class="tab-content">
						<div id="login">   
							<h1><font size="6" color="#ffffff">Welcome Back!</font></h1>
							<form action="/Passport" method="post">
								<div class="field-wrap">
									<input type="text" name="username" autocomplete="off" placeholder="*User Name" ng-model="user" required="required" />
								</div>
							
								<div class="field-wrap">
									<input type="password" name="password" autocomplete="off" placeholder="*Password" required="required"/>
								</div>
								<p class="forgot"><a href="/changePassword">Forgot Password?</a></p>
								<button type="submit" class="button button-block" ng-click="saveCookie()">Log In</button>
							
							</form>
						</div>

						
					</div><!-- /tab-content -->
				</div><!-- /form -->
			</div><!-- /col-sm-6 -->
