<% include ../partials/Submit/mcqForm %>
<div class="col-md-9">
					<div class="container-fluid" id="inside" style="border:1px solid #444242; color: white;">
						<div class="inbox-body">
							<div>
								
								<table class="table table-inbox table-hover">
	                <tbody>
	                  	<tr class="unread">
		                    <td style="width: 50%"><%= survey._id %></td>
										 		<td style="width: 50%"><%= survey.surveyname %></td>	                  	
	                  	</tr>               
	                </tbody>
             		</table>
					      
					      <form action="/SubmitSurvey" method="post">
								<div>
								<% for (index=0;index<survey.question.length;index++) { %>
								<div>
									<p><font color="#28F397" size="4"><%= survey.question[index].que %></font></p>
									<div class="radio">
								    <label><input type="radio" name="<% survey.question[index].que %>" value="ans1"><%= survey.question[index].ans1 %></label>
								  </div>
								  <div class="radio">
								    <label><input type="radio" name="<% survey.question[index].que %>" value="ans2"><%= survey.question[index].ans2 %></label>
								  </div>
								  <div class="radio">
								    <label><input type="radio" name="<% survey.question[index].que %>" value="ans3"><%= survey.question[index].ans3 %></label>
								  </div>
								  <div class="radio">
								    <label><input type="radio" name="<% survey.question[index].que %>" value="ans4"><%= survey.question[index].ans4 %></label>
								  </div>
								  <div class="radio">
								    <label><input type="radio" name="<% survey.question[index].que %>" value="ans5"><%= survey.question[index].ans5 %></label>
								  </div>
								</div>
								<% } %>
								</div>
							  </form>
							</div>

							<div>
								<button type="submit" class="btn btn-default">Register</button>
							</div>

						</div>
					</div>
				</div>
				
			</div>